<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="NavigationService.Views.ModalNavigationPage">
    <ContentPage.Content>
        <StackLayout VerticalOptions="Center">
            <Label HorizontalOptions="Center" Text="Modal Navigation Page" />
            <Label HorizontalOptions="Center" x:Name="NavigationParameterLabel" Text="" />
            <Button Text="Go Back" Clicked="OnNavigateBack"/>
            <Button Text="Push Page" Clicked="OnPushNavigate"/>
            <!--<Button Text="Modal Page" Clicked="OnModalNavigate"/>-->
        </StackLayout>
    </ContentPage.Content>
</ContentPage>